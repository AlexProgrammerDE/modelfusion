{
  "name": "@modelfusion/server",
  "description": "ModelFusion Server",
  "version": "0.0.1",
  "author": "Lars Grammel",
  "license": "MIT",
  "keywords": [
    "ai",
    "multi-modal",
    "modelfusion",
    "fastify",
    "server"
  ],
  "homepage": "https://modelfusion.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lgrammel/modelfusion"
  },
  "bugs": {
    "url": "https://github.com/lgrammel/modelfusion/issues"
  },
  "type": "module",
  "sideEffects": false,
  "private": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "./*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./fastify-plugin": {
      "types": "./fastify-plugin/index.d.ts",
      "import": "./fastify-plugin/index.mjs",
      "module": "./fastify-plugin/index.mjs",
      "require": "./fastify-plugin/index.js"
    }
  },
  "scripts": {
    "lint": "eslint --ext .ts src",
    "clean": "rimraf build dist",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir dist/",
    "build:cjs": "tsc --outDir build/cjs/ -p tsconfig.cjs.json && node bin/prepare-cjs.js",
    "dist:copy-files": "copyfiles package.json README.md LICENSE dist",
    "dist": "npm run clean && npm run lint && npm run build && npm run dist:copy-files"
  },
  "dependencies": {},
  "devDependencies": {
    "modelfusion": "^0.53.2",
    "fastify": "^4.0.0"
  },
  "peerDependencies": {
    "modelfusion": "^0.53.2",
    "fastify": "^4.0.0"
  }
}
