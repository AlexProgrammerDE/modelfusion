{
  "name": "ai-utils",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "./*"
  ],
  "exports": {
    "./provider/openai": {
      "types": "./provider/openai/index.d.ts",
      "import": "./provider/openai/index.js",
      "require": "./provider/openai/index.cjs"
    },
    "./text/generate": {
      "types": "./text/generate/index.d.ts",
      "import": "./text/generate/index.js",
      "require": "./text/generate/index.cjs"
    }
  },
  "author": "Lars Grammel",
  "keywords": [],
  "scripts": {
    "clean": "rimraf build dist",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir dist/",
    "build:cjs": "tsc --outDir build/cjs/ -p tsconfig.cjs.json && node bin/move-cjs-to-dist.js",
    "dist:copy-files": "copyfiles package.json dist",
    "dist": "npm run clean && npm run build && npm run dist:copy-files"
  },
  "devDependencies": {
    "@tsconfig/recommended": "1.0.2",
    "@types/node": "20.2.3",
    "copyfiles": "2.4.1",
    "rimraf": "5.0.1",
    "typescript": "5.0.4",
    "zod": "3.21.4"
  }
}
