{
  "name": "ai-utils.js",
  "description": "A toolkit for building AI apps, chatbots and agents",
  "version": "0.0.7",
  "author": "Lars Grammel",
  "license": "MIT",
  "keywords": [
    "llm",
    "embedding",
    "openai",
    "cohere",
    "huggingface",
    "stability-ai",
    "gpt-3",
    "gpt-4",
    "whisper",
    "chatbot",
    "ai"
  ],
  "homepage": "https://ai-utils.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lgrammel/ai-utils.js"
  },
  "bugs": {
    "url": "https://github.com/lgrammel/ai-utils.js/issues"
  },
  "type": "module",
  "sideEffects": false,
  "private": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "./*"
  ],
  "exports": {
    "./provider/cohere": {
      "types": "./provider/cohere/index.d.ts",
      "import": "./provider/cohere/index.js",
      "require": "./provider/cohere/index.cjs"
    },
    "./provider/huggingface": {
      "types": "./provider/huggingface/index.d.ts",
      "import": "./provider/huggingface/index.js",
      "require": "./provider/huggingface/index.cjs"
    },
    "./provider/openai": {
      "types": "./provider/openai/index.d.ts",
      "import": "./provider/openai/index.js",
      "require": "./provider/openai/index.cjs"
    },
    "./provider/stability": {
      "types": "./provider/stability/index.d.ts",
      "import": "./provider/stability/index.js",
      "require": "./provider/stability/index.cjs"
    },
    "./internal": {
      "types": "./internal/index.d.ts",
      "import": "./internal/index.js",
      "require": "./internal/index.cjs"
    },
    "./prompt": {
      "types": "./prompt/index.d.ts",
      "import": "./prompt/index.js",
      "require": "./prompt/index.cjs"
    },
    "./run": {
      "types": "./run/index.d.ts",
      "import": "./run/index.js",
      "require": "./run/index.cjs"
    },
    "./text": {
      "types": "./text/index.d.ts",
      "import": "./text/index.js",
      "require": "./text/index.cjs"
    },
    "./util": {
      "types": "./util/index.d.ts",
      "import": "./util/index.js",
      "require": "./util/index.cjs"
    },
    "./vector-db": {
      "types": "./vector-db/index.d.ts",
      "import": "./vector-db/index.js",
      "require": "./vector-db/index.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf build dist",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir dist/",
    "build:cjs": "tsc --outDir build/cjs/ -p tsconfig.cjs.json && node bin/prepare-cjs.js",
    "dist:copy-files": "copyfiles package.json README.md LICENSE dist",
    "dist": "npm run clean && npm run build && npm run dist:copy-files"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.0",
    "eventsource-parser": "1.0.0",
    "js-tiktoken": "1.0.6",
    "secure-json-parse": "2.7.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@tsconfig/recommended": "1.0.2",
    "@types/node": "20.2.5",
    "copyfiles": "2.4.1",
    "rimraf": "5.0.1",
    "typescript": "5.0.4"
  }
}
